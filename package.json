{
  "name": "theme-atom-dark",
  "version": "0.1.0",
  "description": "A one dark import into jupyter lab",
  "author": "burglarbenson",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "style",
    "themePath": "style/index.css"
  },
  "homepage": "https://github.com/burglarbenson/theme-atom-dark",
  "bugs": {
    "url": "https://github.com/burglarbenson/theme-atom-dark/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/burglarbenson/theme-atom-dark.git"
  }
}
